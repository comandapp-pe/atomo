<p id="notice"><%= notice %></p>

<h1>Quotations Layouts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotations_layouts.each do |quotations_layout| %>
      <tr>
        <td><%= link_to 'Show', quotations_layout %></td>
        <td><%= link_to 'Edit', edit_quotations_layout_path(quotations_layout) %></td>
        <td><%= link_to 'Destroy', quotations_layout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

