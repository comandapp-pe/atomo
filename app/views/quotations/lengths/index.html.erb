<p id="notice"><%= notice %></p>

<h1>Quotations Lengths</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotations_lengths.each do |quotations_length| %>
      <tr>
        <td><%= link_to 'Show', quotations_length %></td>
        <td><%= link_to 'Edit', edit_quotations_length_path(quotations_length) %></td>
        <td><%= link_to 'Destroy', quotations_length, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

