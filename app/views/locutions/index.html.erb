<p id="notice"><%= notice %></p>

<h1>Locutions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locutions.each do |locution| %>
      <tr>
        <td><%= link_to 'Show', locution %></td>
        <td><%= link_to 'Edit', edit_locution_path(locution) %></td>
        <td><%= link_to 'Destroy', locution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Locution', new_locution_path %>
